    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <RootNamespace>BD_BACK</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Controllers\BD-BACKController.cs" />
